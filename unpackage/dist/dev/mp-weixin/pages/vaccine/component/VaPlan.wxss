.cost {
  background: #F2F2F2;
  position: fixed;
  text-align: center;
  bottom: 0;
  width: 100%;
}
.cost .moneyNum {
  display: inline-block;
  width: 60%;
}
.cost .vaccBtn {
  color: #fff;
}

